{
  "apiVersion": "v1",
  "kind": "Secret",
  "metadata": {
    "name": "<key-name>-secret"
  },
  "data": {
    ".dockerconfigjson": {
      "auths": {
        "${registry-url}": {
          "username": "${access_key}",
          "password": "${secret_key}",
          "auth": "BASE64(${access_key}:${secret_key})"
        }
      }
    }
  },
  "type": "kubernetes.io/dockerconfigjson"
}
